<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.seven.androidfirstbookalldemo.MainActivity">

    <Button
        android:id="@+id/button_main_GoToC2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="C2" />

    <Button
        android:id="@+id/button_main_GoToC3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="C3"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/button_main_GoToC2"
        android:layout_toEndOf="@+id/button_main_GoToC2"
        android:layout_marginLeft="13dp"
        android:layout_marginStart="13dp" />

    <Button
        android:id="@+id/button_main_GoToC4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="C4"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/button_main_GoToC3"
        android:layout_toEndOf="@+id/button_main_GoToC3"
        android:layout_marginLeft="18dp"
        android:layout_marginStart="18dp" />

    <Button
        android:id="@+id/button_main_GoToC5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="C5"
        android:layout_marginTop="13dp"
        android:layout_below="@+id/button_main_GoToC2"
        android:layout_toLeftOf="@+id/button_main_GoToC3"
        android:layout_toStartOf="@+id/button_main_GoToC3" />

    <Button
        android:id="@+id/button_main_GoToC6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="C6"
        android:layout_alignBaseline="@+id/button_main_GoToC5"
        android:layout_alignBottom="@+id/button_main_GoToC5"
        android:layout_alignLeft="@+id/button_main_GoToC3"
        android:layout_alignStart="@+id/button_main_GoToC3" />

    <Button
        android:id="@+id/button_main_GoToC7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="C7"
        android:layout_alignBaseline="@+id/button_main_GoToC6"
        android:layout_alignBottom="@+id/button_main_GoToC6"
        android:layout_alignLeft="@+id/button_main_GoToC4"
        android:layout_alignStart="@+id/button_main_GoToC4" />

    <Button
        android:id="@+id/button_main_GoToC8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="C8"
        android:layout_below="@+id/button_main_GoToC5"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="15dp" />

    <Button
        android:id="@+id/button_main_GoToC9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="C9"
        android:layout_alignBaseline="@+id/button_main_GoToC8"
        android:layout_alignBottom="@+id/button_main_GoToC8"
        android:layout_alignLeft="@+id/button_main_GoToC6"
        android:layout_alignStart="@+id/button_main_GoToC6" />

    <Button
        android:id="@+id/button_main_GoToC10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="C10"
        android:layout_alignBaseline="@+id/button_main_GoToC9"
        android:layout_alignBottom="@+id/button_main_GoToC9"
        android:layout_alignLeft="@+id/button_main_GoToC7"
        android:layout_alignStart="@+id/button_main_GoToC7" />

    <Button
        android:id="@+id/button_main_GoToC12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="C12"
        android:layout_below="@+id/button_main_GoToC8"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="19dp" />

</RelativeLayout>
